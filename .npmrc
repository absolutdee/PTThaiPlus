# .npmrc - NPM Configuration for FitConnect Frontend

# Use legacy peer deps to resolve dependency conflicts
legacy-peer-deps=true

# Use npm registry
registry=https://registry.npmjs.org/

# Disable audit warnings during install (optional)
audit=false

# Enable progress bar
progress=true

# Set log level
loglevel=warn

# Save exact versions (optional)
save-exact=false

# Engine strict mode (optional)
engine-strict=false