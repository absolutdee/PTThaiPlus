{
  "name": "fitconnect-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "FitConnect - แพลตฟอร์มค้นหาเทรนเนอร์ออกกำลังกาย",
  "homepage": "https://absolutdee.github.io/PTThaiPlus",
  "author": "FitConnect Team",
  "license": "MIT",
  "keywords": [
    "fitness",
    "trainer",
    "thailand",
    "health",
    "exercise",
    "personal-trainer"
  ],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.0.0",
    "bootstrap": "^5.3.0",
    "lucide-react": "^0.533.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "recharts": "^3.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.22.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^2.8.0",
    "sass": "^1.62.0",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "deploy": "npm run build && gh-pages -d build",
    "predeploy": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "proxy": "http://localhost:3001"
}
