  "homepage": "https://absolutdee.github.io/FitConnect",
  "bugs": {
    "url": "https://github.com/absolutdee/FitConnect/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/absolutdee/FitConnect.git"
  },
  "license": "MIT",
  "author": {
    "name": "absolutdee",
    "email": "absolutdee@example.com",
    "url": "https://github.com/absolutdee"
  },
  "keywords": [
    "fitness",
    "trainer",
    "personal-trainer",
    "react",
    "fitness-app",
    "thai",
    "bootstrap",
    "mobile-app",
    "responsive"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,scss,md,json}\"",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "analyze": "npm run build && npx serve -s build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "serve": "npx serve -s build -l 3000",
    "clean": "rm -rf node_modules package-lock.json build",
    "fresh-install": "npm run clean && npm install"
  },
  "dependencies": {
    "axios": "^1.5.0",
